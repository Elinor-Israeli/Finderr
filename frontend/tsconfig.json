{
  "compilerOptions": {
    "module": "ESNext",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "types": [
      "vite/client", "node"
    ],
  },
  "target": "esnext",
  "lib": [
    "esnext",
    "dom"
  ],
  "strict": true,
  "esModuleInterop": true,
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
